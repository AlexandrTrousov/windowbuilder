			<v8:Description>Ламинация</v8:Description>
			<v8:Формула>// на входе в переменной obj имеем:
// {
//	ox,			-	объект характеристики текущей продукции
//	elm,		-	текущий элемент, обычно - профиль
//  cnstr,		-	номер конструкции или элемента для привязки параметров
//  inset,		-	вставка элемента или допвставка
//	row_ins,	-	строка спецификации вставки, из которой произведён вызов формулы
//	row_spec	-	строка спецификации изделия, в которой заполняем количество
//	}

const {elm, row_spec, row_ins, len} = obj;

// номенклатура элемента						
const {nom, clr} = elm;

// получаем ссылки параметров
const prm_in = this.params.get(0).param;
const prm_out = this.params.get(1).param;

// с каких сторон ламинировать?
const {is_in, is_out} = clr.sides;

// получаем погонную площадь
let s = 0;
nom.extra_fields.forEach((row) =&gt; {
	is_in &amp;&amp; row.property == prm_in &amp;&amp; (s += row.value);
	is_out &amp;&amp; row.property == prm_out &amp;&amp; (s += row.value);
});

// рассчитываем количество в строке спецификации
row_spec.len = (len + row_ins.sz) * row_ins.coefficient * s;</v8:Формула>
